<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.paas.dao.sys_authority.ISys_authorityMapper"> 
	<select id="selectsys_authorityById" parameterType="String" resultType="Sys_authority">
		SELECT * FROM sys_authority WHERE ID=#{id}
	</select>


	<delete id="deletesys_authority" parameterType="String"> 
		DELETE FROM sys_authority WHERE ID=#{id}
	</delete>

	<select id="selectsys_authorityByParam" parameterType="java.util.Map" resultType="Sys_authority">
		SELECT *
		FROM sys_authority
		<trim prefix="WHERE" prefixOverrides="AND|OR">
			<if test="id != null"> AND ID =#{id} </if>
			<if test="mem_id != null"> AND MEM_ID =#{mem_id} </if>
			<if test="autsign != null"> AND AUTSIGN =#{autsign} </if>
			<if test="nm_t != null"> AND NM_T =#{nm_t} </if>
			<if test="status != null"> AND STATUS =#{status} </if>
			<if test="c_dtFrom != null"> AND unix_timestamp(C_DT) &gt;=unix_timestamp(#{c_dtFrom}) </if>
			<if test="c_dtTo != null"> AND unix_timestamp(C_DT) &lt;=unix_timestamp(#{c_dtTo}) </if>
			<if test="c_dt != null"> AND C_DT =#{c_dt} </if>
			<if test="up_dtFrom != null"> AND unix_timestamp(UP_DT) &gt;=unix_timestamp(#{up_dtFrom}) </if>
			<if test="up_dtTo != null"> AND unix_timestamp(UP_DT) &lt;=unix_timestamp(#{up_dtTo}) </if>
			<if test="up_dt != null"> AND UP_DT =#{up_dt} </if>
		</trim>  
		ORDER BY ID DESC 
		LIMIT #{fromPage},#{toPage} 
	</select>

	<select id="selectCountsys_authorityByParam" parameterType="java.util.Map" resultType="int">
		SELECT COUNT(ID) 
		FROM sys_authority 
		<trim prefix="WHERE" prefixOverrides="AND|OR">
			<if test="id != null"> AND ID =#{id} </if>
			<if test="mem_id != null"> AND MEM_ID =#{mem_id} </if>
			<if test="autsign != null"> AND AUTSIGN =#{autsign} </if>
			<if test="nm_t != null"> AND NM_T =#{nm_t} </if>
			<if test="status != null"> AND STATUS =#{status} </if>
			<if test="c_dtFrom != null"> AND unix_timestamp(C_DT) &gt;=unix_timestamp(#{c_dtFrom}) </if>
			<if test="c_dtTo != null"> AND unix_timestamp(C_DT) &lt;=unix_timestamp(#{c_dtTo}) </if>
			<if test="c_dt != null"> AND C_DT =#{c_dt} </if>
			<if test="up_dtFrom != null"> AND unix_timestamp(UP_DT) &gt;=unix_timestamp(#{up_dtFrom}) </if>
			<if test="up_dtTo != null"> AND unix_timestamp(UP_DT) &lt;=unix_timestamp(#{up_dtTo}) </if>
			<if test="up_dt != null"> AND UP_DT =#{up_dt} </if>
		</trim>  
	</select>

	<update id="updatesys_authority" parameterType="Sys_authority">
		UPDATE sys_authority 
		<trim prefix="SET" suffixOverrides=",">
			<if test="id != null">ID=#{id}, </if>
			<if test="mem_id != null">MEM_ID=#{mem_id}, </if>
			<if test="autsign != null">AUTSIGN=#{autsign}, </if>
			<if test="nm_t != null">NM_T=#{nm_t}, </if>
			<if test="status != null">STATUS=#{status}, </if>
			<if test="c_dt != null">C_DT=#{c_dt}, </if>
			<if test="up_dt != null">UP_DT=#{up_dt}, </if>
		</trim>
		WHERE ID=#{id}
	</update>

	<insert id="addsys_authority" parameterType="Sys_authority">
		INSERT INTO sys_authority 
		<trim prefix="(" suffix=")" suffixOverrides="," >
			<if test="mem_id != null">MEM_ID, </if>
			<if test="autsign != null">AUTSIGN, </if>
			<if test="nm_t != null">NM_T, </if>
			<if test="status != null">STATUS, </if>
			<if test="c_dt != null">C_DT, </if>
			<if test="up_dt != null">UP_DT, </if>
		</trim>
		<trim prefix="VALUES (" suffix=")" suffixOverrides="," >
			<if test="mem_id != null">#{mem_id}, </if>
			<if test="autsign != null">#{autsign}, </if>
			<if test="nm_t != null">#{nm_t}, </if>
			<if test="status != null">#{status}, </if>
			<if test="c_dt != null">#{c_dt}, </if>
			<if test="up_dt != null">#{up_dt}, </if>
		</trim>
		<selectKey order="AFTER" resultType="long" keyProperty="id" > 
			SELECT LAST_INSERT_ID()
		</selectKey> 
	</insert>
</mapper>
